        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://intelligence.csd.auth.gr/ontologies/tosca/port"/>
                <owl:someValuesFrom rdf:resource="https://intelligence.csd.auth.gr/ontologies/tosca/tosca.datatypes.network.PortDef"/>
            </owl:Restriction>
