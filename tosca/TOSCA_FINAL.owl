        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://intelligence.csd.auth.gr/ontologies/tosca/initiator"/>
                        <owl:hasValue>peer</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://intelligence.csd.auth.gr/ontologies/tosca/initiator"/>
                        <owl:hasValue>source</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://intelligence.csd.auth.gr/ontologies/tosca/initiator"/>
                        <owl:hasValue>target</owl:hasValue>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
